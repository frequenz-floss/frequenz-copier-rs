use pyo3::prelude::*;


#[pyfunction]
fn hello_world() -> PyResult<String> {
    Ok("Hello world!".to_string())
}


#[pymodule]
fn _rust_backend(m: &Bound<'_, PyModule>) -> PyResult<()> {
    m.add_function(wrap_pyfunction!(hello_world, m)?)?;
    Ok(())
}
